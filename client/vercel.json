{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "routes": [
      { "src": "/[^.]+", "dest": "/", "status": 200 }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "s-maxage=0" }
        ]
      },
      {
        "source": "/(.*).(js|css|svg|png|jpg|jpeg|gif|webp|ico|json)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }